{
  "name": "amazing-react-charts",
  "version": "0.2.48",
  "description": "An amazing react charts package based in echarts",
  "author": "Leonardo Filipe Rigon",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "keywords": [
    "nginformatica",
    "charts",
    "echarts",
    "react",
    "canvas"
  ],
  "scripts": {
    "start": "tsc --emitDeclarationOnly && docz dev",
    "build:ts": "tsc --emitDeclarationOnly",
    "build": "yarn build:ts && yarn build:babel",
    "build:babel": "babel src --out-dir dist --extensions \".ts,.tsx\"",
    "serve": "docz build && docz serve",
    "pre-publish": "yarn lint --fix && yarn tsc",
    "docz:build": "docz build",
    "lint": "eslint 'src/**/*.ts?(x)'",
    "prepack": "yarn build"
  },
  "dependencies": {
    "date-fns": "2.4.1",
    "echarts": "4.8.0",
    "echarts-for-react": "2.0.15-beta.1",
    "ramda": "0.26.1"
  },
  "devDependencies": {    
    "react-dom": "16.12.0",
    "@babel/cli": "7.10.0",
    "@babel/core": "7.9.0",
    "@babel/plugin-transform-runtime": "7.9.0",
    "@babel/preset-env": "7.9.0",
    "@babel/preset-react": "7.9.0",
    "@babel/preset-typescript": "7.9.0",
    "@types/echarts": "4.6.1",
    "@types/ramda": "0.26.36",
    "@types/react": "16.9.14",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-imports": "2.0.0",
    "brazilian-values": "0.7.0",
    "docz": "2.1.1",
    "eslint": "6.7.2",
    "eslint-config-keepfy": "7.0.0",
    "eslint-plugin-import": "2.18.2",
    "typescript": "3.9.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
